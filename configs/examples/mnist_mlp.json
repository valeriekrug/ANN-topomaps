{
  "model": "MNIST-MLP",
  "data": "MNIST",
  "layers": [
    1
  ],
  "distance_metric": "cosine",
  "nap_params": {
    "n_random_examples": 1000,
    "error_mode": "binary_contrast"
  },
  "topomap_params": {
    "neuron_activations": {
      "use_naps": false,
      "n_random_examples": 1000,
      "balance_by_label": false,
      "use_correct_only": true
    },
    "methods": ["UMAP_PSO"]
  },
  "plot_params": {
    "plot_inputs": true,
    "avg_inputs": false,
    "ordered": true,
    "scale": 2.0,
    "use_title": false,
    "plot_individually": false
  }
}